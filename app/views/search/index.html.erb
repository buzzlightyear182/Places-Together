Welcome! This is the Search results page

<ul>
<% if @search_results != nil %>
<p>Here are your results</p>
  <% @search_results.each do |result| %>
  <li>
    <%= link_to result.name, trip_path(:id => result.id) %>
  </li>
  <% end %>
<% else %>
  <%= "Sorry no results found!" %>
<% end %>
</ul>

<button><%=link_to "Create a new trip", new_trip_path%></button>

<% if user_signed_in? && current_user %>
<button>
  <%= link_to "Edit my profile", edit_profile_path(id: current_user.id) %>
</button>
  <% end %>
